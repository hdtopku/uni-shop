{
  "pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/a/a",
      "aliasPath": "/",
      "style": {
        "navigationBarTitleText": "."
      }
    },
    {
      "path": "pages/test/test",
      "aliasPath": "/test/:id",
      "style": {
        "navigationBarTitleText": "",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/am/am",
      "aliasPath": "/c/:code",
      "meta": {
        "keepScroll": true
      },
      "style": {
        "navigationBarTitleText": "."
      }
    },
    {
      "path": "pages/jet/jet",
      "aliasPath": "/j/:code",
      "style": {
        "navigationBarTitleText": "",
        "enablePullDownRefresh": false
      }
    }, {
      "path": "pages/to/to",
      "aliasPath": "/to/:code",
      "style": {
        "navigationBarTitleText": "",
        "enablePullDownRefresh": false
      }
    }
  ],
  "globalStyle": {
    "navigationBarBackgroundColor": "#000",
    "navigationStyle": "custom", // 隐藏系统导航栏
    "navigationBarTextStyle": "white", // 状态栏字体为white或black
    // #ifdef H5
    "rpxCalcMaxDeviceWidth": 0, // ipad适配
    // #endif
    "app-plus": {
      "titleView": false
    }
  },
  "easycom": {
    "^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue",
    "ui-(.*)": "@/ui/components/ui-$1/ui-$1.vue",
    "app-(.*)": "@/app/components/app-$1/app-$1.vue"
  }
}
